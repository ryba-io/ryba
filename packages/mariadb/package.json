{
  "name": "@rybajs/mariadb",
  "version": "0.0.1",
  "description": "MariaDB deployment",
  "keywords": [
    "database",
    "mariadb",
    "nosql"
  ],
  "author": "David Worms <david@adaltas.com>",
  "homepage": "http://www.ryba.io",
  "license": "MIT",
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "peerDependencies": {
    "masson": "^0.2.0"
  },
  "dependencies": {
    "mixme": "^0.5.9",
    "nikita": "1.0.0-alpha.3",
    "regexp-quote": "~0.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://wdavidw@github.com/ryba-io/ryba.git"
  },
  "scripts": {
    "test": "mocha 'test/server/configure.coffee'",
    "test_fn": "mocha 'test/server/functional_tests.coffee'"
  },
  "bugs": {
    "url": "https://github.com/ryba-io/ryba/issues"
  },
  "devDependencies": {
    "coffeescript": "^2.7.0",
    "mocha": "10.2.0",
    "should": "~13.2.3",
    "ssh2-they": "^2.2.1"
  },
  "mocha": {
    "throw-deprecation": true,
    "recursive": true,
    "reporter": "spec",
    "timeout": 40000,
    "require": [
      "should",
      "coffeescript/register"
    ]
  }
}
